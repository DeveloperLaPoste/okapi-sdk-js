{
  "name": "okapi-sdk",
  "version": "0.0.1",
  "description": "Okapi client SDK for nodejs and the browser",
  "main": "lib/okapi-sdk.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "browserify -d -r ./lib/okapi-sdk.js:okapi-sdk -o demo/okapi-sdk.js",
    "lint": "eslint .",
    "test": "mocha",
    "cover": "istanbul cover _mocha",
    "coveralls": "npm run cover -- -- -R spec && cat ./dist/coverage/lcov.info | coveralls && rm -rf ./dist/coverage",
    "testserver": "./bin/testserver"
  },
  "bin": {
    "testserver": "./bin/testserver"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:DeveloperLaPoste/okapi-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/DeveloperLaPoste/okapi-sdk-js/issues"
  },
  "author": "Developer <developer@laposte.io> (https://developer.laposte.fr/)",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.4.6",
    "lodash": "4.15.0",
    "path-to-regexp": "1.5.3",
    "request": "2.74.0"
  },
  "devDependencies": {
    "body-parser": "1.15.2",
    "browserify": "13.1.0",
    "chai": "3.5.0",
    "coveralls": "2.11.14",
    "eslint": "3.5.0",
    "eslint-config-nodejs": "1.1.0",
    "eslint-plugin-import": "1.15.0",
    "express": "4.14.0",
    "formidable": "1.0.17",
    "istanbul": "0.4.5",
    "mocha": "3.0.2",
    "portfinder": "1.0.7",
    "rimraf": "2.5.4"
  }
}
